        {
          "_name": "contacts1",
          "_sourceTheme": "successm4",
          "_customHTML": "<section data-bs-version=\"5.1\" group=\"Contacts\" class=\"contacts1\" data-bg-video=\"{{bg.type == 'video' && bg.value.url}}\" mbr-class=\"{\n      'mbr-fullscreen': fullScreen,\n      'mbr-parallax-background': bg.parallax}\">\n      <mbr-parameters>\n          <header>Size</header>\n          <input type=\"checkbox\" title=\"Full Screen\" name=\"fullScreen\">\n          <input type=\"checkbox\" title=\"Full Width\" name=\"fullWidth\">\n          <input type=\"range\" inline title=\"Top\" name=\"paddingTop\" min=\"0\" max=\"10\" step=\"1\" value=\"5\" condition=\"fullScreen == false\">\n          <input type=\"range\" inline title=\"Bottom\" name=\"paddingBottom\" min=\"0\" max=\"10\" step=\"1\" value=\"5\" condition=\"fullScreen == false\">\n          <input type=\"range\" name=\"textWidth\" title=\"Width\" min=\"4\" max=\"8\" value=\"4\" step=\"1\">\n          <header>Show/Hide</header>\n          <input type=\"checkbox\" title=\"Show List Title\" name=\"showListTitle\" checked>\n          <input type=\"color\" title=\"Marker Color\" name=\"currentColor\" value=\"#edefeb\">\n          <input type=\"range\" title=\"List Width\" name=\"listWidth\" min=\"0\" max=\"12\" step=\"1\" value=\"4\" condition=\"fullScreen == false\">\n          <input type=\"range\" name=\"item\" title=\"Item Count\" min=\"0\" max=\"3\" value=\"3\" step=\"1\">\n          <input type=\"checkbox\" title=\"Show Icons\" name=\"showSocial\" condition=\"item == 3\">\n          <input type=\"range\" title=\"Icon Count\" name=\"iconCount\" min=\"0\" max=\"5\" step=\"1\" value=\"3\" condition=\"showSocial && item == 3\">    \n          <input type=\"color\" title=\"Border Icon Color\" name=\"borderIconColor\" value=\"#9e9e9e\" condition=\"showSocial &amp;&amp; iconCount > 0 &amp;&amp; item == 3\">\n          <input type=\"color\" title=\"Icon Color\" name=\"iconColor\" value=\"#000\" condition=\"showSocial &amp;&amp; iconCount > 0 &amp;&amp; item == 3\">\n          <input type=\"color\" title=\"Icon Color Hover\" name=\"iconColorHover\" value=\"#f23801\" condition=\"showSocial &amp;&amp; iconCount > 0 &amp;&amp; item == 3\">\n          <header>Background</header>\n          <fieldset type=\"background\" name=\"bg\" parallax>\n    <input type=\"image\" title=\"Image\" value=\"@ADDONS_PATH@/successm4/components/_images/background1.jpg\" parallax>\n    <input type=\"color\" title=\"Color\" value=\"#232323\" selected>\n    <input type=\"video\" title=\"Video\" value=\"https://vimeo.com/428046504\">\n          </fieldset>\n        <header condition=\"bg.type === 'video'\">Fallback Image</header>\n        <input type=\"image\" title=\"Fallback Image\" value=\"@ADDONS_PATH@/successm4/components/_images/background1.jpg\" name=\"fallBackImage\" condition=\"bg.type === 'video'\">\n\n          <input type=\"checkbox\" title=\"Overlay\" name=\"overlay\" condition=\"bg.type !== 'color'\" checked>\n          <input type=\"color\" title=\"Overlay Color\" name=\"overlayColor\" value=\"#ffffff\" condition=\"overlay && bg.type !== 'color'\">\n          <input type=\"range\" inline title=\"Opacity\" name=\"overlayOpacity\" min=\"0\" max=\"1\" step=\"0.1\" value=\"0.8\" condition=\"overlay && bg.type !== 'color'\">\n      </mbr-parameters>\n    <div class=\"mbr-fallback-image disabled\" mbr-if=\"bg.type == 'video'\"></div>\n\n      <div class=\"mbr-overlay\" mbr-if=\"overlay && bg.type !== 'color'\" opacity=\"{{overlayOpacity}}\" bg-color=\"{{overlayColor}}\"></div>\n      <div mbr-class=\"{'container': !fullWidth, 'container-fluid': fullWidth}\">\n          <div class=\"row\">\n    <div class=\"col-md-12 col-lg-{{textWidth}}\">\n        <div class=\"text-wrapper\">\n           <p class=\"list-title mbr-fonts-style\" mbr-theme-style=\"display-5\" mbr-if=\"item > 0\" data-app-selector=\".list-title\"><b>Personal Info</b></p>\n            <p class=\"mbr-fonts-style mbr-section-tag mb-0\" mbr-theme-style=\"display-7\" mbr-if=\"item > 0\" data-app-selector=\".mbr-section-tag, .mbr-section-link, .mbr-social-linkes\"><b>&nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; Adres</b></p>\n            <p class=\"mbr-section-link mbr-fonts-style\" mbr-theme-style=\"display-7\" mbr-if=\"item > 0\" data-app-selector=\".mbr-section-link\">&nbsp; &nbsp; &nbsp; &nbsp; &nbsp;Haverstraatpassage 64\n<br>&nbsp; 7511 EX Enschede<br><a href=\"https://maps.app.goo.gl/niog1635dEr1kQAp6\" class=\"text-white\">(Routebeschrijving in google maps)</a></p>\n           <p class=\"mbr-fonts-style mbr-section-tag mb-0\" mbr-theme-style=\"display-7\" mbr-if=\"item > 1\" data-app-selector=\".mbr-section-tag, .mbr-section-link, .mbr-social-linkes\"><b>Email</b></p>\n            <p class=\"mbr-section-link mbr-fonts-style\" mbr-theme-style=\"display-7\" mbr-if=\"item > 1\" data-app-selector=\".mbr-section-link\">Info@wizardofink.nl</p>\n           <p class=\"mbr-fonts-style mbr-section-tag mb-0\" mbr-theme-style=\"display-7\" mbr-if=\"item > 2\" data-app-selector=\".mbr-section-tag, .mbr-section-link, .mbr-social-linkes\"><b>Telefoon</b><br><b>+31613378058</b><br><b><i>(uitstekend bereikbaar via Whatsapp)</i></b></p>\n          <div class=\"mbr-social-linkes align-left\" data-counters=\"false\" mbr-if=\"showSocial &amp;&amp; item > 2\">\n            <a class=\"iconfont-wrapper ml-0\" href=\"https://mobiri.se\" target=\"_blank\" mbr-if=\"iconCount > 0\">\n      <span mbr-icon class=\"p-2 ml-0 mbr-iconfont socicon-facebook socicon\"></span>\n            </a>\n            <a class=\"iconfont-wrapper\" href=\"https://mobiri.se\" target=\"_blank\" mbr-if=\"iconCount > 1\">\n      <span mbr-icon class=\"p-2 mbr-iconfont socicon-linkedin socicon\"></span>\n            </a>\n            <a class=\"iconfont-wrapper\" href=\"https://mobiri.se\" target=\"_blank\" mbr-if=\"iconCount > 2\">\n      <span mbr-icon class=\"p-2 mbr-iconfont socicon-twitter socicon\"></span>\n            </a>\n            <a class=\"iconfont-wrapper\" href=\"https://mobiri.se\" target=\"_blank\" mbr-if=\"iconCount > 3\">\n      <span mbr-icon class=\"p-2 mbr-iconfont socicon-youtube socicon\"></span>\n            </a>\n            <a class=\"iconfont-wrapper\" href=\"https://mobiri.se\" target=\"_blank\" mbr-if=\"iconCount > 4\">\n      <span mbr-icon class=\"p-2 mbr-iconfont socicon-pinterest socicon\"></span>\n            </a>\n        </div>\n    </div>\n          </div>\n          <div class=\"col-12 col-lg-{{listWidth}}\">\n             <p class=\"list-title mbr-fonts-style\" mbr-theme-style=\"display-5\" mbr-if=\"showListTitle\" data-app-selector=\".list-title\"><b>List Title</b></p>\n    <ul class=\"list mbr-fonts-style\" mbr-theme-style=\"display-7\" data-app-selector=\".list\" data-multiline mbr-article><i><b>&nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; Openingstijden&nbsp;</b></i><br><i>&nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp;Ma: Gesloten\n</i><div><i>&nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; Di:    10:00 - 18:00\n</i></div><div><i>&nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp;Wo:  10:00 - 18:00\n</i></div><div><i>&nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp;Do:   10:00 - 21:00\n</i></div><div><i>&nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; Vrij:  10:00 - 18:00\n</i></div><div><i>&nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; Za:    10:00 - 18:00\n</i></div><div><i>&nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; Zo:    10:00 - 18:00</i></div></ul>\n          </div>\n          <div class=\"col-12 col-lg-{{listWidth}}\">\n             <p class=\"list-title mbr-fonts-style\" mbr-theme-style=\"display-5\" mbr-if=\"showListTitle\" data-app-selector=\".list-title\"><b>List Title</b></p>\n    <ul class=\"list mbr-fonts-style\" mbr-theme-style=\"display-5\" data-app-selector=\".list\" data-multiline mbr-article></ul>\n          </div>\n      </div>\n  </div>\n  </section>",
          "_styles": {
            "& when not (@fullScreen)": {
              "padding-top": "(@paddingTop * 1rem)",
              "padding-bottom": "(@paddingBottom * 1rem)"
            },
            "& when (@bg-type = 'image')": {
              "background-image": "url(@bg-value)"
            },
            "& when (@bg-type = 'color')": {
              "background-color": "@bg-value"
            },
            "ul": {
              "list-style": "none",
              "margin": "0",
              "padding-left": "1.6rem"
            },
            "li": {
              "position": "relative",
              "margin-bottom": "0.625rem",
              "display": "inline-block"
            },
            "ul li::before": {
              "position": "absolute",
              "left": "-1.6rem",
              "top": "0.625rem",
              "content": "\"\"",
              "width": "10px",
              "height": "10px",
              "border-radius": "50%",
              "background-color": "@currentColor"
            },
            ".mbr-section-subtitle": {
              "margin-bottom": "0.625rem"
            },
            ".list": {
              "&-title": {
                "margin-top": "1.5625rem",
                "margin-bottom": "1.125rem"
              },
              "color": "#f7f7f7",
              "text-align": "right"
            },
            ".mbr-social-linkes": {
              "margin-top": "0.5rem",
              "& a": {
                "display": "inline-block",
                "color": "@iconColor"
              },
              "& span": {
                "display": "flex",
                "align-items": "center",
                "justify-content": "center",
                "border-radius": "50%",
                "border": "1px solid @borderIconColor",
                "width": "36px",
                "height": "36px",
                "margin-left": "0.4375rem",
                "margin-right": "0.4375rem",
                "transition": "0.3s ease-out",
                "&:hover": {
                  "border-color": "@iconColorHover",
                  "color": "@iconColorHover"
                }
              }
            },
            ".mbr-section-link": {
              "margin-bottom": "30px",
              "text-align": "right"
            },
            ".mbr-section-tag": {
              "color": "#9e9e9e",
              "margin-bottom": "0.4375rem",
              "text-align": "left"
            },
            ".mbr-section-tag, .mbr-section-link": {
              "text-align": "center"
            },
            ".mbr-section-tag, .mbr-section-link, .mbr-social-linkes": {
              "text-align": "right",
              "color": "#f7f7f7"
            },
            ".mbr-fallback-image.disabled": {
              "display": "none"
            },
            ".mbr-fallback-image": {
              "display": "block",
              "background-size": "cover",
              "background-position": "center center",
              "width": "100%",
              "height": "100%",
              "position": "absolute",
              "top": "0",
              "& when (@bg-type = 'video')": {
                "background-image": "url(@fallBackImage)"
              }
            },
            ".list DIV": {
              "text-align": "right"
            }
          },
